import java.util.Scanner;
public class Main
{
	public static void main(String[] args) 
	{
		Scanner dc = new Scanner (System.in);
		 int n1, n2; 
		 
		 System.out.println("BIENVENIDO QUERIDO USUARIO");
		 
		 System.out.print("ingrese el valor #1: ");
			n1=dc.nextInt();
			System.out.print("ingrese el valor #2: ");
			n2=dc.nextInt();
			
		if (n1<n2)
		System.out.println("el numero 1 es menor");
		else 
		System.out.println("el numero 2 menor ");
			
	}
}	
